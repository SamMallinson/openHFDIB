FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      HFDIBDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
stlNames ("sphere");

interpolationSchemes
{
 U cellPointFace;
}

sphere
{
 
 translatingBody
 {
    velocity (-0.025 0 0);
 }
 
 refineMC 5;
 
 U
 {
  BC  noSlip;
 }

}

